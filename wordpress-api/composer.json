{
    "name": "n1k3/wordpress-api",
    "description": "Laravel 5.1 wrapper for the most popular PHP library for use with the Wordpress OAuth REST API.",
    "keywords": [
        "wordpress",
        "oauth",
        "REST",
        "api",
        "laravel"
    ],
    "homepage": "https://github.com/n1k3/wordpress-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Nwankwo",
            "email": "emmanueln.nike@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "n1k3\\WordpressApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "n1k3\\WordpressApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
